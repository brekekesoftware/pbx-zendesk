{
  "name": "Brekeke PBX",
  "author": {
    "name": "Akpesiri Okorigba",
    "email": "esiri92@gmail.com",
    "url": "https://brekeke.com"
  },
  "defaultLocale": "en",
  "private": true,
  "location": {
    "support": {
      "top_bar": {
        "url": "assets/index.html",
        "autoLoad": true,
        "size": {
          "width": "400px",
          "height": "520px"
        }
      }
    }
  },
  "version": "1.0.0",
  "frameworkVersion": "2.0",
  "parameters": [
    {
      "name": "widgetUrl",
      "type": "text",
      "required": true,
      "secure": true,
      "default": "https://brekeke:8443/pbx/etc/widget/zendesk/index.html"
    }
  ]
}
